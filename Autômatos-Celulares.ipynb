      },
      "source": [
        "MYPATH = '/content/drive/My Drive/ProjetoIC/Brodatz/Brodatz_128x128'\n",
        "NEWPATH = '(CAMINHO DA PASTA A ESCOLHER)'\n",
        "ONLYFILES = [f for f in listdir(MYPATH) if isfile(join(MYPATH, f))]"
      ],
      "execution_count": 17,
      "outputs": []
    },
